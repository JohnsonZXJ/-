<!--pages/friends/friends.wxml-->
<scroll-view scroll-y class="scrollPage">
<cu-custom bgColor="bg-gradual-green" isBack="{{true}}">
  <view slot="content">校友圈</view>
</cu-custom>

<view class="cu-card">
    <view class="cu-item bg-img shadow-blur" style="background-image:url({{item.img}})" bindtap="toChild" data-url="{{item.url}}" wx:for="{{list}}" wx:key='index'>
      <view class="cardTitle">
        {{item.title}}
      </view>
    </view>
  </view>
  <view class="cu-tabbar-height"></view>
  </scroll-view>